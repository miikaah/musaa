{
  "name": "musaa",
  "version": "1.0.0",
  "description": "Musa proxy",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f .env.dev nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env node dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^2.0.0-beta.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "esbuild": "^0.19.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
